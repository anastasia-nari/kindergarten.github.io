ErrorDocument 404 /404.html

RewriteCond %{REQUEST_FILENAME} robots.txt$ [NC]

RewriteCond %{SERVER_PORT} ^80$ [OR]
RewriteCond %{HTTP} =on
RewriteRule ^(.*)$ https://main--site-kindergarten-nzh.netlify.app/$1 [R=301,L]

RewriteEngine On
RewriteCond %{HTTP_HOST} ^www.main--site-kindergarten-nzh.netlify.app [NC]
RewriteRule ^(.*)$ https://main--site-kindergarten-nzh.netlify.app /$1 [R=302,L]
